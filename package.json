{
  "name": "lactaconsejos",
  "version": "6.0.6",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "start:dev": "expo start --config app.dev.json",
    "android": "expo run:android",
    "android:dev": "expo run:android --config app.dev.json",
    "ios": "expo run:ios",
    "ios:dev": "expo run:ios --config app.dev.json",
    "web": "expo start --web",
    "web:dev": "expo start --web --config app.dev.json",
    "lint": "eslint ./src --fix",
    "bump:patch": "node scripts/bump-version.js --type patch",
    "bump:minor": "node scripts/bump-version.js --type minor",
    "bump:major": "node scripts/bump-version.js --type major",
    "bump:dry-run": "node scripts/bump-version.js --dry-run",
    "bump:set": "node scripts/bump-version.js --set",
    "build:dev:android": "eas build --platform android --profile development --non-interactive",
    "build:preview:android": "eas build --platform android --profile preview --non-interactive",
    "build:prod:android": "node scripts/bump-version.js --type patch && eas build --platform android --profile production --non-interactive"
  },
  "dependencies": {
    "@babel/preset-env": "^7.28.3",
    "@babel/preset-react": "^7.27.1",
    "@expo/vector-icons": "^15.0.2",
    "@react-native-masked-view/masked-view": "0.3.2",
    "@react-navigation/bottom-tabs": "^7.4.0",
    "@react-navigation/native": "^7.1.8",
    "@react-navigation/stack": "^7.0.0",
    "@react-spring/web": "^9.7.3",
    "@rneui/base": "^4.0.0-rc.7",
    "@rneui/themed": "^4.0.0-rc.8",
    "@types/react": "~19.1.10",
    "@types/react-dom": "~19.1.7",
    "babel-preset-expo": "^54.0.4",
    "expo": "^54.0.13",
    "expo-constants": "~18.0.9",
    "expo-font": "~14.0.9",
    "expo-linear-gradient": "~15.0.7",
    "expo-linking": "~8.0.8",
    "expo-react-native-skeleton-placeholder": "^0.0.1",
    "expo-router": "~6.0.12",
    "expo-sharing": "~14.0.7",
    "expo-splash-screen": "~31.0.10",
    "expo-status-bar": "~3.0.8",
    "expo-updates": "~29.0.12",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.4",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-paper": "^5.10.6",
    "react-native-render-html": "^6.3.4",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-share": "^10.0.2",
    "react-native-svg": "15.12.1",
    "react-native-vector-icons": "^10.0.0",
    "react-native-web": "^0.21.0",
    "react-svg": "^16.1.27"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@types/node": "^20.11.16",
    "eslint": "^8.57.0",
    "eslint-plugin-autofix": "^1.1.0",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-simple-import-sort": "^12.0.0",
    "prettier": "3.0.3",
    "react-native-svg-transformer": "^1.1.0",
    "typescript": "^5.9.2"
  },
  "private": true
}
